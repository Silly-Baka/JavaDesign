<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="design.ViewController.ImagePreviewViewController"
            prefHeight="400.0" prefWidth="1000.0"
            fx:id="rootPane">
    <TreeView fx:id="treeView">
        <prefWidth>300</prefWidth>
        <prefHeight>600</prefHeight>
    </TreeView>
    <ScrollPane AnchorPane.leftAnchor="300" prefHeight="600">
        <FlowPane fx:id="imageLabelsPane" hgap="10" vgap="10">
            <AnchorPane.leftAnchor>330</AnchorPane.leftAnchor>
            <prefWidth>880</prefWidth>
            <hgap>10</hgap>
            <vgap>10</vgap>
        </FlowPane>
    </ScrollPane>
    <Label fx:id="tipsLabel">
        <prefHeight>30</prefHeight>
        <prefWidth>1200</prefWidth>
        <AnchorPane.topAnchor>600</AnchorPane.topAnchor>
    </Label>
</AnchorPane>
